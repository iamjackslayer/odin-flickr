<%= form_for :user, url:{action: :index}, method: :get do |f| %>
ID: <%= f.text_field :id %>
<%= f.submit :submit %>
<% end %>
<% if defined? @photos %>
	<% @photos.each do |photo| %>
		<%= image_tag("https://farm#{photo.farm}.staticflickr.com/#{photo.server}/#{photo.id}_#{photo.secret}.jpg") %>
	<% end %>
<% end %>