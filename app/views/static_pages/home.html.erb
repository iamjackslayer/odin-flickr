<%= form_for :user, url:{action: :create}, method: :post do |f| %>
ID: <%= f.text_field :id %>
<%= f.submit :submit %>
<% end %>
<% if @photos %>
	<% @photos.each do |photo| %>
		<%= image_tag("https://#{photo.farm}staticflickr.com/#{photo.server}/#{photo.id}_#{photo.secret}.jpg" %>
	<% end %>
<% end %>